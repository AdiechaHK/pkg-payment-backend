    "extra": {
        "laravel": {
            "providers": [
                "Adiechahk\\PaymentBackend\\PaymentServiceProvider"
            ],
            "aliases": {
                "Payment": "Adiechahk\\PaymentBackend\\Facades\\Payment"
            }
        }
    }
}
