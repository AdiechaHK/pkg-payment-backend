{
    "name": "adiechahk/payment-backend",
    "description": ":package_description",
    "license": "MIT",
    "authors": [
        {
            "name": "Adiecha Harikrushna",
            "email": "adiechahari@email.com",
            "homepage": "http://harikrushna.info"
        }
    ],
    "homepage": "https://github.com/adiechahk/payment-backend",
    "keywords": ["Laravel", "PaymentBackend"],
    "require": {
        "illuminate/support": "~9",
        "stripe/stripe-php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Adiechahk\\PaymentBackend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adiechahk\\PaymentBackend\\Tests\\": "tests"
        }
